CREATE TABLE `nomads`.`preferences` (
  `preferences_id` INT NOT NULL AUTO_INCREMENT,
  `country` VARCHAR(45) NOT NULL,
  `cultural` INT(11) NULL,
  `food` INT(11) NULL,
  `outdoors` INT(11) NULL,
  `urban` INT(11) NULL,
  PRIMARY KEY (`preferences_id`, `country`),
  UNIQUE INDEX `preferences_id_UNIQUE` (`preferences_id` ASC),
  UNIQUE INDEX `preferencescol_UNIQUE` (`country` ASC));
  
  